<HomeWindow>:
    name: "home"
    
    FloatLayout:
        cols: 1
        
        FloatLayout:
            size: root.width, root.height/2
            
            canvas.before:
                Rectangle:
                    size: root.width, 3*root.height/4
                    pos: (0, root.height/4)
                    source: "brooke-lark-HlNcigvUi4Q-unsplash.jpg"
                
                Rectangle:
                    size: root.width, root.height/4
                    pos: self.pos
                    source: "drew-beamer-kUHfMW8awpE-unsplash.jpg"
            
            Label: 
                text: "FDS"
                size_hint: 0.1, 0.1
                pos_hint: {"x": 0, "top": 1}
                font_size: (root.width**2 + root.height**2) / 14**4   
                color: 0, 0, 0, 1
                
            Button:
                size_hint: 0.2, 0.1
                pos_hint:{"x":0.55, "top": 1}
                text: "Sign up"
                background_color: 1, 1, 0, 0.2
                on_release:
                    app.root.current = "create"
                    root.manager.transition.direction = "down"
                    
            Button:
                size_hint: 0.2, 0.1
                pos_hint:{"x":0.8, "top":1}
                text: "Login"
                background_color: 1, 1, 0, 0.2
                on_release:
                    app.root.current = "login"
                    root.manager.transition.direction = "up"
        
        Label: 
            text: "For Restaurant Partners"
            size_hint: 0.6, 0.1
            pos_hint: {"x": 0.2, "top": 0.25}
            font_size: (root.width**2 + root.height**2) / 14**4   
            color: 0, 0, 0, 1
            
        Button:
            size_hint: 0.3, 0.08
            pos_hint:{"x":0.1, "top":0.15}
            text: "Register"
            on_release:
                app.root.current = "partnercreate"
                root.manager.transition.direction = "down"
        
        Button:
            size_hint: 0.3, 0.08
            pos_hint:{"x":0.6, "top":0.15}
            text: "Login"
            on_release:
                app.root.current = "partnerlogin"
                root.manager.transition.direction = "up"
        

<CreateAccountWindow>:
    name: "create"

    namee: namee
    email: email
    password: passw

    FloatLayout:
        cols:1
        
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "monika-grabkowska-TAj4X5-eRqE-unsplash.jpg"
        
        FloatLayout:
            size: root.width, root.height/2

            Label:
                text: "Create an Account"
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8}
                text: "Name: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8}
                size_hint: 0.4, 0.12
                id: namee
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8-0.13}
                text: "Email: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8-0.13}
                size_hint: 0.4, 0.12
                id: email
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8-0.13*2}
                text: "Password: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8-0.13*2}
                size_hint: 0.4, 0.12
                id: passw
                multiline: False
                password: True
                font_size: (root.width**2 + root.height**2) / 14**4

        Button:
            pos_hint:{"x":0.3,"y":0.25}
            size_hint: 0.4, 0.1
            font_size: (root.width**2 + root.height**2) / 16**4
            text: "Already have an Account? Log In"
            background_color: 0, 0, 1, 0.3
            color: 1, 1, 1, 1
            on_release:
                root.manager.transition.direction = "left"
                root.login()

        Button:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.15
            text: "Submit"
            font_size: (root.width**2 + root.height**2) / 14**4
            color: 1, 1, 1, 1
            background_color: 0, 0, 1, 0.3
            on_release:
                root.manager.transition.direction = "left"
                root.submit()

